{
    "name": "cms",
    "description": "Infosystems CMS",
    "keywords": ["yii2", "advanced", "CMS"],
    "homepage": "http://www.infosystems.uz/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "http://git.uzcloud.uz/aripdjanov/cms/issues",
        "source": "http://git.uzcloud.uz/aripdjanov/cms"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0.0",
        "yiisoft/yii2": ">=2.0.9",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "mihaildev/yii2-elfinder": "*",
        "mihaildev/yii2-ckeditor": "*",
        "dmstr/yii2-adminlte-asset": "^2.4",
        "la-haute-societe/yii2-save-relations-behavior": "^1.2",
        "codemix/yii2-localeurls": "^1.5",
        "paulzi/yii2-nested-sets": "^1.0",
        "fishvision/yii2-migrate": "*",
        "paulzi/yii2-auto-tree" : "^1.0",
        "abdualiym/yii2-language-class": "^1.0"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "secure-http": false,
        "process-timeout": 1800
    }
}
